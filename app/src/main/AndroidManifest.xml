<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.animalgame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/lionicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.CountDownActivity"></activity>
        <activity android:name=".view.ResultActivity" />
        <activity android:name=".view.RecordActivity" />
        <activity android:name=".view.GameActivity2" />
        <activity android:name=".view.HowToPlayActivity" />
        <activity android:name=".view.GameActivity" />
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>